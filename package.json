    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.2",
    "cookie-parser": "^1.4.7",
    "express-session": "^1.18.1",
    "ioredis": "^5.6.0",
    "nestjs-pino": "^4.3.1",
    "pino-http": "^10.4.0",
