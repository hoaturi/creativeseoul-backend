    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "configPaths": [
      "./dist/config/mikro-orm.config.js",
      "./src/config/mikro-orm.config.ts"
    ]
  },
  "packageManager": "yarn@4.6.0"
